    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-mount": "^3.0.0",
    "koa-rewrite": "^3.0.1",
    "koa-router": "^7.4.0",
    "nanoid": "^1.0.3",
    "nuxt": "^1.4.0",
    "oidc-provider": "^4.0.1",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
